
  <% unless user.admin? %>
  <tr>
    <th><%= user.id %></th>
    <td><%= user.email %></td>
    <td><%= user.username %></td>
    <td><%= user.role %></td>
    <td><%= link_to "Delete", user_path(user), method: :delete, data: {confirm: "Are you sure?"}, class: "btn btn-outline-primary" %></td>
  </tr>
  <% end %>
