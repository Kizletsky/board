
  <% unless user == current_user %>
  <tr id="user-row-<%= user.id %>">
    <td><%= user.email %></td>
    <td><%= user.username %></td>
    <td><%= user.role %></td>
    <td><%= link_to "Show", user_path(user), class: "btn btn-outline-secondary" %>
        <%= link_to "Edit", edit_user_path(user), class: "btn btn-outline-secondary" %>
        <%= link_to "Delete", user_path(user), method: :delete, data: {confirm: "Are you sure?"}, remote: true, class: "btn btn-outline-secondary" %>
    </td>
  </tr>
  <% end %>
